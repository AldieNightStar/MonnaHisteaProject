	(function (Util) {
	    function declick(span) {
	        if (!(span instanceof HTMLElement))
	            return;
	        span.onclick = null;
	        for (let child of Array.from(span.children)) {
	            Util.declick(child);
	        }
	    }
	    Util.declick = declick;
	})(Util || (Util = {}));
