                        timerElement.innerHTML = "â²ï¸" + intervalCount;
                    }
                    s.interval(1000, () => {
                        intervalCount -= 1;
                        if (intervalCount < 1) {
                            if (action)
                                action();
                            Passage.goto(passage, passageArgs);
                        }
                        updateElement();
                    });
                    updateElement();
                }
            });
        }
        Passage.ofChoice = ofChoice;
