        if (isNull(text)) {
            s.title("ðŸ”®Void...ðŸ”®");
        }
        else {
            s.println(text);
        }
        function setPage(p) {
            V.page = p;
            if (V.page < 0)
                V.page = 0;
            if (V.page >= story.length)
                V.page = story.length - 1;
        }
        s.rebutton("<<", () => setPage(V.page - 1));
        s.print(page);
        s.rebutton(">>", () => setPage(V.page + 1));
    });
    
    
