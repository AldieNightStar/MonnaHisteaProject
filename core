            }
            interval(ms, cb) {
                const timer = Engine.Timer.interval(ms, () => {
                    if (!this.isConnected()) {
                        timer.stop();
                        return;
                    }
                    ;
                    cb();
                });
                return timer;
            }
            wait(ms) {
                return new Promise(async (ok) => {
                    await Engine.Timer.wait(ms);
                    if (!this.span.isConnected)
                        return;
                    ok();
                });
            }
